{
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 110,
  "semi": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy", "classProperties"],
  "importOrder": [
    "^node:",
    "^fs$",
    "^path$",
    "<BUILTIN_MODULES>",
    "^express$",
    "^@nestjs/(.*)$",
    "^zod$",
    "^ts-pattern$",
    "<THIRD_PARTY_MODULES>",
    "^src/shared/services$",
    "^src/shared$",
    "^src/types$",
    "^src/utils$",
    "^src/config$",
    "^[.]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
